            </nav>
        </header>
        <div>
          <div class="container shadow-lg mb-5" id="box2">
              <form action="/myProfile/<%= user._id %>?_method=PUT" class="form" id="accountPage" method="POST" >
                  <h2 class="form__title mt-1">Edit page</h2>
                  <div class="form-floating form__input-group mb-3">
                      <input id="username" type="text" autofocus placeholder="Username" name="username" class="form-control" value="<%= user.username %>">
                      <Label for="username">Username</Label>
                      <div class="form__input-error-message mt-1" id="errorMessage"></div>
                  </div>
                  <div class="form-floating form__input-group mb-3">
                    <input class="form-control" id="first" type="text" autofocus placeholder="First name" name="first"  value="<%= user.first %>">
                    <label class="form-label" for="first">First name</label>
                </div>
                <div class="form-floating form__input-group mb-3">
                    <input class="form-control" id="last" type="text" autofocus placeholder="Last name" name="last"  value="<%= user.last %>">
                    <label class="form-label" for="last">Last name</label>
                </div>
                  <div class="form-floating form__input-group mb-3">
                      <input id="email" type="email" autofocus placeholder="Email" name="email" class="form-control" value="<%= user.email %>">
                      <label for="email" class="form-label"> Email</label>
                      <div class="form__input-error-message" id="errorMessage"></div>
                  </div>
                  <div class="form-floating form__input-group mb-3">
                      <input id="password" class="form-control" type="password" autofocus placeholder="Password" name="password" value="<%= user.password %>">
                      <label for="password" class="form-label">Password</label>
                      <div class="form__input-error-message"></div>
                  </div>
                  <div class="form-floating form__input-group mb-3">
                    <input class="form-control" id="number" type="number" autofocus placeholder="Phone Number" name="number"  value="<%= user.number %>">
                    <label class="form-label" for="number">Phone Number</label>
                </div>
                  <button class="btn btn-success btn-lg mb-2" type="submit" id="login"  >Update</button>
                  <p class="form__text mt-1"><a href="/myProfile/<%= user._id %>" class="form__link">Back to account Page </a></p>
              </form>
          </div>
      </div>
    </main>
    <%- include('../partials/footer'); %> 
    
